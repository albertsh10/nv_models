{
    "keys": [ "platform", "model", "precision", "length"],
    "dirs": [ "precision" ],
    "platform": [
        {
            "name": "DGX1",
            "params": "python ./multiproc.py --nproc_per_node 8 ./main.py /imagenet --raport-file raport.json -j5 -p 100 --data-backend pytorch"
        }
    ],
    "model" : [
        {
         "name": "SE-RNXT101-32x4d",
